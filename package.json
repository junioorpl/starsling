{
  "name": "starsling",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "lint:check": "eslint . --ext .ts,.tsx,.js,.jsx --max-warnings 0",
    "lint:fix-all": "eslint . --ext .ts,.tsx,.js,.jsx --fix --max-warnings 0",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write . && eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "check-all": "npm run lint:check && npm run format:check && npm run build",
    "fix-all": "npm run format:fix",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:setup": "tsx scripts/setup-db.ts",
    "docker:dev": "docker-compose up --build",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up --build",
    "docker:down": "docker-compose down",
    "docker:clean": "docker-compose down -v --rmi all",
    "docker:test": "./test-docker.sh"
  },
  "dependencies": {
    "@octokit/auth-app": "^8.1.0",
    "@octokit/rest": "^22.0.0",
    "@types/crypto-js": "^4.2.2",
    "@types/pg": "^8.15.5",
    "better-auth": "^1.3.11",
    "clsx": "^2.1.1",
    "crypto-js": "^4.2.0",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.44.5",
    "inngest": "^3.40.3",
    "lucide-react": "^0.544.0",
    "next": "15.5.3",
    "pg": "^8.16.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "prettier": "^3.6.2",
    "tailwindcss": "^4",
    "tsx": "^4.20.5",
    "typescript": "^5"
  }
}
